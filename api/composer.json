{
  "name": "azdanov/demo-auction",
  "description": "An auction made with PHP Slim and JavaScript React.",
  "type": "project",
  "license": "BSD-3-Clause",
  "config": {
    "process-timeout": 0,
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "php-di/php-di": "^6.3",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.9",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "lctrs/psalm-psr-container-plugin": "^1.7",
    "overtrue/phplint": "^4.0",
    "psalm/plugin-phpunit": "^0.16.1",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "^4.16"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "lint": "phplint",
    "php-cs-fixer": "php-cs-fixer --config=.php-cs-fixer.php --allow-risky=yes",
    "psalm": "psalm",
    "app": "php bin/app.php --ansi"
  }
}
