{
  "name": "azdanov/demo-auction",
  "description": "An auction made with PHP Slim and JavaScript React.",
  "type": "project",
  "license": "BSD-3-Clause",
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "php-di/php-di": "^6.3",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.9",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "app": "php bin/app.php --ansi"
  }
}
